  "scripts": {
    "start": "npm run build && forever start -a --uid bonstrap -e error.log -o server.log server.js",
    "dev": "webpack-dev-server --config config/webpack/webpack.config.dev.js --progress --history-api-fallback --hot ",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib src --copy-files --ignore tests,layouts,app",
    "build:flow": "flow-copy-source -v -i '**/tests/**' -i '**/layouts/**' -i '**/app/**' src lib",
    "production": "NODE_ENV=production npm start",
    "test": "npm run code-style && npm run with-configured-mocha -- --recursive ./src/**/*.spec.js",
    "with-configured-mocha": "NODE_PATH=./src:./assets:./test BABEL_ENV=testing mocha -r mock-local-storage --require ./config/test/testSetup.js --require ignore-styles --require ./config/test/loadermock.js  --compilers js:babel-register,.:mocha-image-compiler",
    "code-style": "npm run eslint /src && npm run flow &&npm run stylelint",
    "stylelint": "./node_modules/.bin/stylelint src/**/*.css",
    "eslint": "./node_modules/.bin/eslint",
    "flow": "./node_modules/.bin/flow",
    "semantic-release": "semantic-release"
  }
